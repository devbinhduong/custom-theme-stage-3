{
  "components/halothemes/gallery/halo-product-paginator": "<div class=\"halo-paginator\">\n\t{{#if page_type '==' 'category'}}\n\t\t{{> components/common/paginator pagination.category total=category.total_products page_type=page_type theme_settings=theme_settings}}\n\t{{/if}}\n\n\t{{#if page_type '==' 'brand'}}\n\t\t{{> components/common/paginator pagination.brand page_type=page_type brand=brand shop_by_brand=shop_by_brand theme_settings=theme_settings}}\n\t{{/if}}\n\n\t{{#if page_type '==' 'search'}}\n\t\t{{> components/common/paginator pagination.product_results total=pagination.product_results.total page_type=page_type theme_settings=theme_settings}}\n\t{{/if}}\n</div>\n",
  "components/common/paginator": "<nav class=\"pagination{{#if theme_settings.halo-product-show-more}} pagination-2{{/if}}\" aria-label=\"pagination\">\n    {{#if page_type '==' 'category'}}\n        {{#if total}}\n            {{#if start}}\n                {{#if end}}\n                    {{#if end '<' total}}\n                        <div class=\"pagination-info\"><span class=\"end\">{{end}}</span> din <span class=\"total\">{{total}}</span> de carti</div>\n                        <div class=\"pagination-total-progress\">\n                            <span class=\"pagination-total-item\"></span>\n                        </div>\n                    {{else}}\n                        <div class=\"pagination-info\"><span class=\"end\">{{end}}</span> din <span class=\"total\">{{total}}</span> de carti</div>\n                        <div class=\"pagination-total-progress\">\n                            <span class=\"pagination-total-item\"></span>\n                        </div>\n                    {{/if}}\n                {{/if}}\n            {{/if}}\n        {{/if}}\n        {{#if theme_settings.halo-product-show-more}}\n            <div class=\"halo-product-show-more\" id=\"listing-showmoreBtn\">\n                {{#if next}}\n                    <a class=\"button button--primary\" href=\"#\">\n                        <span class=\"text\">{{lang 'search.faceted.show-more'}}</span>\n                    </a>\n                {{else}}\n                    <a class=\"button button--primary disable\" href=\"#\">\n                        <span class=\"text\">{{lang 'common.no_more_products'}}</span>\n                    </a>\n                {{/if}}\n            </div> \n        {{/if}}\n    {{/if}}\n    {{#if page_type '==' 'brand'}}\n        {{#if links.length}}\n            {{#each shop_by_brand}}\n                {{#if name '==' ../brand.name}}\n                    <div class=\"pagination-info\">Items <span class=\"start\"></span> to <span class=\"end\"></span> of <span class=\"total\">{{count}}</span> total</div>\n                    <div class=\"pagination-total-progress\">\n                        <span class=\"pagination-total-item\"></span>\n                    </div>\n                {{/if}}\n            {{/each}}\n        {{/if}}\n        {{#if theme_settings.halo-product-show-more}}\n            <div class=\"halo-product-show-more\" id=\"listing-showmoreBtn\">\n                {{#if next}}\n                    <a class=\"button button--primary\" href=\"#\">\n                        <span class=\"text\">{{lang 'search.faceted.show-more'}}</span>\n                    </a>\n                {{else}}\n                    <a class=\"button button--primary disable\" href=\"#\">\n                        <span class=\"text\">{{lang 'common.no_more_products'}}</span>\n                    </a>\n                {{/if}}\n            </div> \n        {{/if}}\n    {{/if}}\n    {{#if page_type '==' 'search'}}\n        <div class=\"pagination-info\">Items <span class=\"start\"></span> to <span class=\"end\"></span> of <span class=\"total\">{{total}}</span> total</div>\n        <div class=\"pagination-total-progress\">\n            <span class=\"pagination-total-item\"></span>\n        </div>\n        {{#if theme_settings.halo-product-show-more}}\n            <div class=\"halo-product-show-more\" id=\"listing-showmoreBtn\">\n                {{#if next}}\n                    <a class=\"button button--primary\" href=\"#\">\n                        <span class=\"text\">{{lang 'search.faceted.show-more'}}</span>\n                    </a>\n                {{else}}\n                    <a class=\"button button--primary disable\" href=\"#\">\n                        <span class=\"text\">{{lang 'common.no_more_products'}}</span>\n                    </a>\n                {{/if}}\n            </div> \n        {{/if}}\n    {{/if}}\n    <ul class=\"pagination-list\">\n        {{#if previous}}\n            <li class=\"pagination-item pagination-item--previous\">\n                <a class=\"pagination-link\"\n                   href=\"{{previous}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.previous'}}\"\n                >\n                    <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"8\" height=\"17\" viewBox=\"0 0 8 17\" fill=\"none\">\n                      <path fill-rule=\"evenodd\" clip-rule=\"evenodd\" d=\"M6.19937 8.49981L0.511719 1.39024L1.4878 0.609375L7.4878 8.10937L7.4878 8.89024L1.48781 16.3902L0.511719 15.6094L6.19937 8.49981Z\" fill=\"#252525\"/>\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n\n        {{#each links}}\n            <li class=\"pagination-item {{#if this.number '==' ../current}}pagination-item--current{{/if}}\">\n                <a class=\"pagination-link\"\n                   href=\"{{this.url}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   {{#if this.number '==' ../current}}aria-current=\"page\"{{/if}}\n                   aria-label=\"{{lang 'common.paginator.page_of' current=this.number total=../links.length}}\"\n                >\n                    {{this.number}}\n                </a>\n            </li>\n        {{/each}}\n\n        {{#if next}}\n            <li class=\"pagination-item pagination-item--next\">\n                <a class=\"pagination-link\"\n                   href=\"{{next}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.next'}}\"\n                >\n                    <svg xmlns=\"http://www.w3.org/2000/svg\" width=\"8\" height=\"17\" viewBox=\"0 0 8 17\" fill=\"none\">\n                      <path fill-rule=\"evenodd\" clip-rule=\"evenodd\" d=\"M6.19937 8.49981L0.511719 1.39024L1.4878 0.609375L7.4878 8.10937L7.4878 8.89024L1.48781 16.3902L0.511719 15.6094L6.19937 8.49981Z\" fill=\"#252525\"/>\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n"
}